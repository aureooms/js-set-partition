{
  "name": "@combinatorics/set-partition",
  "description": "Set partition algorithms for JavaScript",
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "author": "make-github-pseudonymous-again",
  "homepage": "https://computational-combinatorics.github.io/set-partition",
  "repository": {
    "type": "git",
    "url": "https://github.com/computational-combinatorics/set-partition"
  },
  "bugs": {
    "url": "https://github.com/computational-combinatorics/set-partition/issues"
  },
  "keywords": [
    "choose",
    "partition",
    "set"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "install-hooks": "husky install",
    "lint": "xo",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "dependencies": {
    "@iterable-iterator/list": "^0.0.2",
    "@iterable-iterator/map": "^0.1.0",
    "@iterable-iterator/select": "^0.0.1"
  },
  "devDependencies": {
    "@aureooms/js-red-black-tree": "9.0.0",
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@babel/register": "7.13.16",
    "@commitlint/cli": "12.1.4",
    "@iterable-iterator/iter": "^0.0.2",
    "@iterable-iterator/range": "^1.0.0",
    "@iterable-iterator/slice": "^0.0.1",
    "@js-library/commitlint-config": "0.0.4",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.1.0",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.7.2",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "6.0.0",
    "np": "7.5.0",
    "pinst": "2.1.6",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7",
    "total-order": "3.0.1",
    "xo": "0.40.1"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": [
            "defaults",
            "maintained node versions"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "sourceMaps": "inline",
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ]
      },
      "cover": {},
      "development": {
        "sourceMaps": "inline",
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ]
      },
      "production": {
        "sourceMaps": "inline",
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ]
      }
    }
  },
  "prettier": {
    "trailingComma": "all"
  },
  "xo": {
    "prettier": true,
    "ignores": [
      "lib"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "doc/**"
        ],
        "env": "browser"
      }
    ]
  }
}
